{

    "name": "AdBlocker",
    "version": "1.0.0",
    "description": "Blocks ads from appearing on your website",
    "permissions": ["webRequest", "webRequestBlocking", "<all_urls>"],
    "background": {
        "scripts": ["background.js"]
    },
    "icons": {
        "16": "images/logo_16x16.png",
        "48": "images/logo_48x48.png",
        "128": "images/logo_128x128.png"
    },
    "manifest_version": 2
}